{
    "name": "browscap/browscap-bundle",
    "description": "Allows you to get browser information",
    "keywords": ["browscap", "browser"],
    "homepage": "https://github.com/browscap/BrowscapBundle",
    "type": "symfony-bundle",
    "require": {
        "php": ">=5.3.3",
        "browscap/browscap-php": "~2.0"
    },
    "require-dev": {
        "symfony/symfony": ">=2.1,<=2.5"
    },
    "license": "MIT",
    "minimum-stability": "dev",
    "authors": [
        {
            "name": "Joshua Estes",
            "email": "Joshua.Estes@ScenicCityLabs.com",
            "homepage": "http://joshuaestes.me",
            "role": "developer"
        }
    ],
    "support": {
        "issues": "https://github.com/browscap/BrowscapBundle/issues"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    },
    "autoload": {
        "psr-0": { "Browscap\\BrowscapBundle": "" }
    },
    "target-dir": "Browscap/BrowscapBundle"
}
