{
  "name": "browscap/browscap-bundle",
  "description": "Allows you to get browser information",
  "keywords": [
    "browscap",
    "browser"
  ],
  "homepage": "https://github.com/browscap/BrowscapBundle",
  "type": "symfony-bundle",
  "require": {
    "php": "^5.3.3 || ^7.0",
    "symfony/symfony": "^2.3 || ^3.0",
    "symfony/framework-bundle": "^2.3 || ^3.0",
    "browscap/browscap-php": "^2.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^4.8.36 || ^5.7.23 || ^6.0.0"
  },
  "license": "MIT",
  "authors": [
    {
      "name": "Joshua Estes",
      "email": "Joshua.Estes@ScenicCityLabs.com",
      "homepage": "http://joshuaestes.me",
      "role": "developer"
    }
  ],
  "support": {
    "issues": "https://github.com/browscap/BrowscapBundle/issues"
  },
  "autoload": {
    "psr-4": {
      "Browscap\\BrowscapBundle\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Browscap\\BrowscapBundle\\Tests\\": "tests/"
    }
  }
}
